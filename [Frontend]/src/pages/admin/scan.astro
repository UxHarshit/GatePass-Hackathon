---
import ScanPage from '@/components/Admin/scan/page';
import PassPage from '@/components/Pass/pass';
import Layout from '@/layouts/Layout.astro';
import "@/styles/global.css";
const x_email = Astro.cookies.get("x-team-email") || null;
if (!x_email) {
	Astro.redirect("/");
}
export const prerender = false;


---

<Layout title="Scan Admin - HackLiet">
	<ScanPage client:load />
</Layout>
